<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppBaseTheme" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
    <style name="AppBaseTheme2" parent="@android:style/Theme.Holo.Light.NoActionBar" />
    <style name="AppTheme2" parent="@style/AppBaseTheme2">
        <item name="android:buttonStyle">@style/ButtonStyle</item>
        <item name="android:windowBackground">@color/window_background</item>
    </style>
    <style name="ButtonStyle" parent="@android:style/Widget.Holo.Light.Button">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:minHeight">@dimen/pay_40_dp</item>
    </style>
    <style name="TextStyle_18sp_black_alpha87">
        <item name="android:textSize">@dimen/pay_18_sp</item>
        <item name="android:textColor">@color/color_black_87</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_16sp_black_alpha90">
        <item name="android:textSize">@dimen/pay_16_sp</item>
        <item name="android:textColor">@color/color_black_90</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_14sp_black_alpha57">
        <item name="android:textSize">@dimen/pay_14_sp</item>
        <item name="android:textColor">@color/color_black_57</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_16sp_brown">
        <item name="android:textSize">@dimen/pay_16_sp</item>
        <item name="android:textColor">@color/color_brown</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_10sp_white">
        <item name="android:textSize">@dimen/pay_10_sp</item>
        <item name="android:textColor">@color/color_white_100</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_12sp_brown">
        <item name="android:textSize">@dimen/pay_12_sp</item>
        <item name="android:textColor">@color/color_brown</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_10sp_gray">
        <item name="android:textSize">@dimen/pay_10_sp</item>
        <item name="android:textColor">@color/color_gray</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_13sp_black_alpha26">
        <item name="android:textSize">@dimen/pay_13_sp</item>
        <item name="android:textColor">@color/color_black_26</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_12sp_black_alpha26">
        <item name="android:textSize">@dimen/pay_12_sp</item>
        <item name="android:textColor">@color/color_black_26</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="TextStyle_11sp_black_alpha26">
        <item name="android:textSize">@dimen/pay_11_sp</item>
        <item name="android:textColor">@color/color_black_26</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="AppBaseTheme2.TransParent" parent="@style/AppBaseTheme2">
        <item name="android:buttonStyle">@style/ButtonStyle</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="TextStyle_16sp_black_alpha87">
        <item name="android:textSize">@dimen/pay_16_sp</item>
        <item name="android:textColor">@color/color_black_87</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="loading_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="translucent">
        <item name="android:windowBackground">@color/translucent_background</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="MessageTextStyle">
        <item name="android:textSize">@dimen/pay_12_sp</item>
        <item name="android:textColor">@color/color_black_26</item>
        <item name="android:singleLine">false</item>
    </style>
    <style name="CouponValueStyle">
        <item name="android:textSize">33.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="CouponUnitStyle">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
</resources>
